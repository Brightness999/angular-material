  "devDependencies": {
    "@types/jest": "26.0.15",
    "eslint": "7.12.1",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-jsdoc": "30.7.3",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-security": "1.4.0",
